<h1 id="welcome">Welcome to Decryptsy</h1>


<h2>Cryptsy Resistance Levels</h2>

Search Table: <input ng-model="query">

<div class = "pad table-scroll">
<div class="table-responsive">
<table class="table table-striped table-hover">
	<thead>
		<th>
			<a href="" ng-click="orderByField='primary'; reverseSort = !reverseSort">
			Primary
			</a>
		</th>
		<th>
			<a href="" ng-click="orderByField='secondary'; reverseSort = !reverseSort">
			Secondary
			</a>
		</th>
		<th>
			<a href="" ng-click="orderByField='last_trade'; reverseSort = !reverseSort">
			Last Trade
			</a>
		</th>
		<th>
			<a href="" ng-click="orderByField='volume'; reverseSort = !reverseSort">
			Volume
			</a>
		</th>
		<th>
			<a href="" ng-click="orderByField='last_trade_time'; reverseSort = !reverseSort">
			Last Trade Time
			</a>
		</th>		<th>
			<a href="" ng-click="orderByField='halfWall'; reverseSort = !reverseSort">
			BTC to 50%
			</a>
		</th>
		<th>
			<a href="" ng-click="orderByField='doubleWall'; reverseSort = !reverseSort">
			BTC to 100%
			</a>
		</th>
		<th>
			<a href="" ng-click="orderByField='secondWall'; reverseSort = !reverseSort">
			BTC to 125%
			</a>
		</th>
		<th>
			<a href="" ng-click="orderByField='thirdWall'; reverseSort = !reverseSort">
			BTC to 150%
			</a>
		</th>
		<th>
			<a href="" ng-click="orderByField='fourthWall'; reverseSort = !reverseSort">
			BTC to 200%
			</a>
	</th>
	</thead>
	<tbody>
		<tr ng-repeat="coinpair in coinpairs | orderBy:orderByField:reverseSort |filter:query" ng-click="showCoinpair(coinpair.id)">
			<td>{{coinpair.primary}}</td>
			<td>{{coinpair.secondary}}</td>
			<td>{{coinpair.last_trade}}</td>
			<td>{{coinpair.volume}}</td>
			<td>{{coinpair.last_trade_time | date: 'MM-dd HH:mm:ss Z'}}</td>
			<td>{{coinpair.halfWall}}</td>
			<td>{{coinpair.doubleWall}}</td>
			<td>{{coinpair.secondWall}}</td>
			<td>{{coinpair.thirdWall}}</td>
			<td>{{coinpair.fourthWall}}</td>
		</tr>
	</tbody>
</table>
</div>
</div>

